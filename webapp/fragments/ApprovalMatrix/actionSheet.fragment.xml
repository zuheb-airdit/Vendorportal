<ActionSheet id="actionSheet"
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	core:require="{ MessageToast: 'sap/m/MessageToast' }"
	title="Choose Your Action"
	showCancelButton="true"
	placement="Bottom">
	<Button
		text="Discard All Changes"
		press="onDiscardAll" />
	<Button
		text="Cancel"
		press="onCloseActionfrag" />
	
</ActionSheet>