<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.ui.layout.form"
    xmlns:l="sap.ui.layout"
>
    <Dialog
        id="previewDialogs"
        title="Form Preview"
        contentWidth="65%"
        contentHeight="auto"
    >
    <VBox id="previewVBox">
        <f:SimpleForm
            id="SimpleFormChangeColumn_oneGrousp234s"
            editable="true"
            layout="ColumnLayout"
            columnsM="2"
            columnsL="2"
            columnsXL="2"
        >
            <f:content>
                <Label
                    text="Vendor Organisation Name"
                    required="true"
                />
                <Text
                    text="{agpFormData>/GENERAL_INFORMATION/VENDOR_INFORMATION/VENDOR_ORGANISATION_NAME}"
                >
                    <layoutData>
                        <l:GridData span="XL12 L12 M12 S12" />
                    </layoutData>
                </Text>

                <Label text="Contact Person" />
                <Text
                    text="{agpFormData>/GENERAL_INFORMATION/VENDOR_INFORMATION/CONTACT_PERSON}"
                />

                <Label text="Priority" />
                <Text
                    text="{agpFormData>/GENERAL_INFORMATION/VENDOR_INFORMATION/PRIORITY}"
                />
            </f:content>
        </f:SimpleForm>
        <f:SimpleForm
            id="SimpleFormChangeColumn_oneGroup234s"
            editable="true"
            layout="ColumnLayout"
            title="Complete Office Address"
            columnsM="2"
            columnsL="2"
            columnsXL="2"
            class="sapUiTinyMarginTop"
            objectBindings="{ path: 'agpFormData>/GENERAL_INFORMATION/VENDOR_ADDRESS' }"
        >
            <f:content>
                <Label
                    text="Street"
                    required="true"
                />
                <Text
                    text="{agpFormData>/GENERAL_INFORMATION/VENDOR_ADDRESS/STREET}"
                />

                <Label
                    text="Street 2"
                    required="true"
                />
                <Text
                    text="{agpFormData>/GENERAL_INFORMATION/VENDOR_ADDRESS/STREET2}"
                />

                <Label
                    text="Street 3"
                    required="true"
                />
                <Text
                    text="{agpFormData>/GENERAL_INFORMATION/VENDOR_ADDRESS/STREET3}"
                />

                <Label
                    text="City"
                    required="true"
                />
                <Text
                    text="{agpFormData>/GENERAL_INFORMATION/VENDOR_ADDRESS/CITY}"
                />

                <Label text="District" />
                <Text
                    text="{agpFormData>/GENERAL_INFORMATION/VENDOR_ADDRESS/DISTRICT}"
                />

                <Label
                    text="Pincode"
                    required="true"
                />
                <Text
                    text="{agpFormData>/GENERAL_INFORMATION/VENDOR_ADDRESS/POSTAL_CODE}"
                />

                <Label
                    text="State"
                    required="true"
                />
                <Text
                    text="{agpFormData>/GENERAL_INFORMATION/VENDOR_ADDRESS/STATE}"
                />

                <Label text="Country" />
                <Text
                    text="{agpFormData>/GENERAL_INFORMATION/VENDOR_ADDRESS/COUNTRY}"
                />

                <Label
                    text="Email Address"
                    required="true"
                />
                <Text
                    text="{agpFormData>/GENERAL_INFORMATION/VENDOR_ADDRESS/EMAIL}"
                />

                <Label text="Email Address for Payment" />
                <Text
                    text="{agpFormData>/GENERAL_INFORMATION/VENDOR_ADDRESS/EMAIL_PAYMENT}"
                />

                <Label
                    text="Telephone No"
                    required="true"
                />
                <Text
                    text="{agpFormData>/GENERAL_INFORMATION/VENDOR_ADDRESS/TELEPHONE}"
                />

                <Label text="Mobile Number (if any)" />
                <Text
                    text="{agpFormData>/GENERAL_INFORMATION/VENDOR_ADDRESS/CONTACT_NO}"
                />
            </f:content>
        </f:SimpleForm>
        <f:SimpleForm
            id="SimpleFormChangeColumn_oneGrokup23h4"
            editable="true"
            layout="ColumnLayout"
            title="Bank Details"
            class="sapUiTinyMarginTop"
            columnsM="2"
            columnsL="2"
            columnsXL="2"
        >
            <f:content>
                <Label text="Bank Name" />
                <Text
                    text="{agpFormData>/FINANCIAL_INFORMATION/PRIMARY_BANK_DETAILS/BANK_NAME}"
                />

                <Label text="Bank Branch" />
                <Text
                    text="{agpFormData>/FINANCIAL_INFORMATION/PRIMARY_BANK_DETAILS/BRANCH_NAME}"
                />

                <Label text="IFSC Code" />
                <Text
                    text="{agpFormData>/FINANCIAL_INFORMATION/PRIMARY_BANK_DETAILS/IFSC_CODE}"
                />

                <Label text="Bank Account Number" />
                <Text
                    text="{agpFormData>/FINANCIAL_INFORMATION/PRIMARY_BANK_DETAILS/ACCOUNT_NO}"
                />

                <Label text="Account Type" />
                <Text
                    text="{agpFormData>/FINANCIAL_INFORMATION/PRIMARY_BANK_DETAILS/ACCOUNT_TYPE}"
                />
            </f:content>
        </f:SimpleForm>

        <f:SimpleForm
            id="SimpleFormChangeColumn_oneGrsoup23ui4"
            editable="true"
            layout="ColumnLayout"
            columnsM="2"
            columnsL="2"
            columnsXL="2"
            class="sapUiTinyMarginTop"
        >
            <f:content>
                <Label text="LST No" />
                <Text
                    text="{agpFormData>/FINANCIAL_INFORMATION/PRIMARY_BANK_DETAILS/LST_NO}"
                />

                <Label text="CST No" />
                <Text
                    text="{agpFormData>/FINANCIAL_INFORMATION/PRIMARY_BANK_DETAILS/CST_NO}"
                />

                <Label text="Due Diligence required" />
                <Text
                    text="{agpFormData>/FINANCIAL_INFORMATION/PRIMARY_BANK_DETAILS/DUE_DILIGENCE_REQUIRED}"
                />

                <Label
                    text="GST No"
                    required="true"
                />
                <Text
                    text="{agpFormData>/FINANCIAL_INFORMATION/PRIMARY_BANK_DETAILS/GST_NO}"
                />

                <Label text="Approver" />
                <Text
                    text="{agpFormData>/FINANCIAL_INFORMATION/PRIMARY_BANK_DETAILS/APPROVER}"
                />

                <Label
                    text="PAN No"
                    required="true"
                />
                <Text
                    text="{agpFormData>/FINANCIAL_INFORMATION/PRIMARY_BANK_DETAILS/PAN_NO}"
                />

                <Label text="MSME Category (if applicable)" />
                <Text
                    text="{agpFormData>/FINANCIAL_INFORMATION/PRIMARY_BANK_DETAILS/MSME_CATEGORY}"
                />

                <Label text="Due Diligence request number" />
                <Text
                    text="{agpFormData>/FINANCIAL_INFORMATION/PRIMARY_BANK_DETAILS/DUE_DELI_NUM}"
                />

                <Label text="Vendor Account Group" />
                <Text
                    text="{agpFormData>/FINANCIAL_INFORMATION/PRIMARY_BANK_DETAILS/STATE}"
                />
            </f:content>
        </f:SimpleForm>
</VBox>
